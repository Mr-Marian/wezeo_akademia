{
  "name": "wezeo_akademia",
  "version": "1.0.0",
  "description": "\"# My New Project\"",
  "main": "postcss.config.js",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev:tailwind": "tailwindcss -i ./src/input.css -o ./dist/output.css --watch",
    "dev:server": "live-server dist/",
    "copy:html": "cp public/index.html dist/index.html",
    "watch:html": "nodemon --watch public/ --ext html --exec \"npm run copy:html\"",
    "dev": "npm run copy:html && npm-run-all -p dev:tailwind dev:server watch:html",
    "build": "npm-run-all copy:html build:css",
    "build:css": "postcss src/input.css -o dist/output.css",
    "start": "live-server dist/"
},
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "chokidar-cli": "^3.0.0",
    "cpx": "^1.5.0",
    "live-server": "^1.2.2",
    "nodemon": "^3.1.10",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "postcss-import": "^16.1.1",
    "tailwindcss": "^3.4.17"
  },
  "dependencies": {
    "caniuse-lite": "^1.0.30001741",
    "tailwind-scrollbar-hide": "^4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mr-Marian/wezeo_akademia.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Mr-Marian/wezeo_akademia/issues"
  },
  "homepage": "https://github.com/Mr-Marian/wezeo_akademia#readme"
}
